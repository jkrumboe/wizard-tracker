# Your Appwrite games collection ID (found in your Appwrite console)
VITE_APPWRITE_GAMES_COLLECTION_ID=ID_OF_YOUR_GAMES_COLLECTION

# App version for build information
VITE_APP_VERSION=1.1.11.2
