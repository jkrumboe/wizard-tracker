{
  "name": "wizard-tracker-backend",
  "version": "1.15.11",
  "description": "Backend API for Wizard Tracker game",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:local": "cross-env MONGO_URI=mongodb://localhost:27017/dev nodemon server.js",
    "test": "cross-env NODE_ENV=test jest --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "elo:calculate": "node scripts/calculate-initial-elo.js",
    "elo:calculate:dry": "node scripts/calculate-initial-elo.js --dry-run --verbose",
    "elo:dev": "cross-env MONGO_URI=mongodb://localhost:27017/dev node scripts/calculate-initial-elo.js --verbose",
    "db:copy-prod": "node scripts/copy-prod-to-dev.js",
    "postgres:test": "node scripts/test-postgres-connection.js",
    "prisma:setup": "node scripts/setup-postgres.js",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "prisma:format": "prisma format",
    "migrate:backfill-users": "node scripts/backfill-users.js",
    "migrate:backfill-users:dry": "node scripts/backfill-users.js --dry-run",
    "migrate:backfill-identities": "node scripts/backfill-identities.js",
    "migrate:backfill-identities:dry": "node scripts/backfill-identities.js --dry-run",
    "migrate:backfill-friends": "node scripts/backfill-friends.js",
    "migrate:backfill-friends:dry": "node scripts/backfill-friends.js --dry-run",
    "migrate:backfill-all": "node scripts/backfill-users.js && node scripts/backfill-identities.js && node scripts/backfill-friends.js && node scripts/backfill-games.js && node scripts/backfill-game-events.js",
    "migrate:backfill-all:dry": "node scripts/backfill-users.js --dry-run && node scripts/backfill-identities.js --dry-run && node scripts/backfill-friends.js --dry-run && node scripts/backfill-games.js --dry-run && node scripts/backfill-game-events.js --dry-run",
    "migrate:backfill-games": "node scripts/backfill-games.js",
    "migrate:backfill-games:dry": "node scripts/backfill-games.js --dry-run",
    "migrate:backfill-games:wizard": "node scripts/backfill-games.js --type wizard",
    "migrate:backfill-games:table": "node scripts/backfill-games.js --type table",
    "migrate:backfill-events": "node scripts/backfill-game-events.js",
    "migrate:backfill-events:dry": "node scripts/backfill-game-events.js --dry-run",
    "migrate:benchmark": "node scripts/benchmark-db.js"
  },
  "keywords": [
    "express",
    "mongodb",
    "jwt",
    "bcrypt"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^7.5.0",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.6.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.22.0",
    "supertest": "^6.3.3"
  }
}
