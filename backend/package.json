{
  "name": "wizard-tracker-backend",
  "version": "1.15.2",
  "description": "Backend API for Wizard Tracker game",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:local": "cross-env MONGO_URI=mongodb://localhost:27017/dev nodemon server.js",
    "test": "cross-env NODE_ENV=test jest --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "elo:calculate": "node scripts/calculate-initial-elo.js",
    "elo:calculate:dry": "node scripts/calculate-initial-elo.js --dry-run --verbose",
    "elo:dev": "cross-env MONGO_URI=mongodb://localhost:27017/dev node scripts/calculate-initial-elo.js --verbose",
    "db:copy-prod": "node scripts/copy-prod-to-dev.js"
  },
  "keywords": [
    "express",
    "mongodb",
    "jwt",
    "bcrypt"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^7.5.0",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.6.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
